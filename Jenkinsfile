pipeline {
    agent any
    parameters {
      choice choices: ['CAMEROON', 'TOGO', 'SENEGAL', 'USA'], description: 'What is your country?', name: 'COUNTRY'
    }

    stages {
        stage('build') {
            steps {
                echo 'Hello World'
            }
        }
        



        stage('test') {
            steps {
                echo 'Hello World'
            }
        }





        stage('deploy') {
            steps {
                echo 'Hello World'
            }
        }


    }
}
        
