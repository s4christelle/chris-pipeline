    agent any
    parameters {
      choice choices: ['CAMEROON', 'TOGO', 'SENEGAL', 'USA'], description: 'What is your country?', name: 'COUNTRY'
      choice choices: ['DOLLAR', 'EURO', 'FCFA'], description: 'CHOOSE YOUR CURRENCY', name: 'MONEY'
    }
      
    stages {
        stage('build') {
            steps {
                echo 'Hello World'
            }
        }
        



        stage('test') {
            steps {
                echo 'Hello World'
            }
        }





        stage('deploy') {
            steps {
                echo 'Hello World'
            }
        }


    }
}
        
